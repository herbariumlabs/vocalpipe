{
    "name": "vocalpipe",
    "version": "1.0.0",
    "description": "Advanced multilingual voice and text AI bot with dual language support",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc && npm run db:generate",
        "dev": "ts-node src/index.ts",
        "start": "node dist/index.js",
        "watch": "tsc --watch",
        "dev:watch": "nodemon --exec ts-node src/index.ts",
        "clean": "rimraf dist",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "format": "prettier --write src/**/*.ts",
        "db:generate": "prisma generate && echo 'âœ… Prisma client generated in dist/generated/prisma'",
        "db:migrate": "prisma migrate dev",
        "db:migrate:deploy": "prisma migrate deploy",
        "db:studio": "prisma studio",
        "db:seed": "ts-node prisma/seed.ts",
        "db:reset": "prisma migrate reset",
        "db:push": "prisma db push",
        "db:pull": "prisma db pull"
    },
    "keywords": [
        "telegram",
        "bot",
        "ai",
        "voice",
        "text",
        "multilingual",
        "speech-to-text",
        "text-to-speech",
        "translation",
        "openai",
        "bhashini"
    ],
    "author": "Rahul Barman",
    "license": "MIT",
    "dependencies": {
        "@langchain/core": "^0.3.0",
        "@langchain/openai": "^0.3.0",
        "@prisma/client": "^6.13.0",
        "dotenv": "^16.4.5",
        "fluent-ffmpeg": "^2.1.2",
        "node-fetch": "^3.3.2",
        "openai": "^4.103.0",
        "posthog-node": "^5.1.1",
        "telegraf": "^4.16.3",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/fluent-ffmpeg": "^2.1.24",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.24",
        "@types/uuid": "^9.0.8",
        "@typescript-eslint/eslint-plugin": "^7.1.1",
        "@typescript-eslint/parser": "^7.1.1",
        "eslint": "^8.57.0",
        "jest": "^29.7.0",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "prisma": "^6.13.0",
        "rimraf": "^5.0.5",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.4.2"
    },
    "engines": {
        "node": ">=18.0.0"
    }
}
